    <query language="SQL"><![CDATA[SELECT
                o.orderId,
                o.orderTotal,
                o.order_date,
                c.customerID,
                c.firstName,
                c.lastName,
                c.email,
                c.mobileNumber,
                c.address,
                od.orderDetailId,
                p.productID,
                p.productName,
                p.productPrice,
                od.quantity,
                (od.quantity * p.productPrice) AS totalPrice
            FROM Orders o
            JOIN customer c ON o.customer_id = c.customerID
            JOIN order_detail od ON o.orderId = od.order_id
            JOIN product p ON od.product_id = p.productID
            WHERE o.orderId = (SELECT MAX(orderId) FROM orders)]]></query>
